<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       <record id="saham_tabungan_form_view" model="ir.ui.view">
           <field name="name">saham.tabungan.form.view</field>
           <field name="model">ksp.tabungan</field>
           <field name="inherit_id" ref="ksp_tabungan.ksp_tabungan_form_view"/>
           <field name="arch" type="xml">

               <xpath expr="//notebook" position="inside">
                   <page string="Saham">
                       <group>
                           <field name="saham_count"/>
                            <field name="saham_current_value"/>
                       </group>
                   </page>
               </xpath>

           </field>
       </record>
    </data>
</odoo>