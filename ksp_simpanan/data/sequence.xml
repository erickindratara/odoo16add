<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_simpanan" model="ir.sequence">
            <field name="name">Sequence Simpanan</field>
            <field name="code">seq.simpanan</field>
            <field name="prefix">SMP/%(year)s/</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_simpanan_line" model="ir.sequence">
            <field name="name">Sequence Simpanan Line</field>
            <field name="code">seq.simpanan.line</field>
            <field name="prefix">SMP-LINE/%(year)s/</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>